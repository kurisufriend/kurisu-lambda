INCLUDE:./lib/aids.klambda

(def a (id (id "desu")))
(spit
	("lol swej" (+ 9000 1) a)
)
(spit ((cond (= "10" (+ 1 9)) "same" "not same")))
(cond ("", (+ 0 1))
	(id spit ("one"))
	(id spit ("zero"))
)

(spit ("how old are you? > "))
(def age (conv number (input)))
(spit 
	(
		(cond (> age 18)
			"adult"
			"not adult"
		)
	)
)

(defun loop
	(id all
		(id id spit ($1))
		(id id cond (id id id = $1 $2)
			$1
			(id id id loop (id id id id + $1 1) $2)
		)
	)
)

(loop 0 5)

(thesis)