(def a (id (id "desu")))
(spit
	("lol swej" (+ 9000 1) a))
(spit (cond (= "10" (+ 1 9)) "same" "not same"))
(cond ("", (+ 0 1))
	(id spit "one")
	(id spit "zero"))

(spit "how old are you? > ")
(def age (conv number (input)))
(spit
	(cond (> age 18)
		"adult (hag)"
		"not adult (uoh)"))

(loop 0 100 
	(id spit (cond (% i 15) (cond (% i 5) (cond (% i 3) i "fizz") "buzz") "fizzbuzz")))

(def moeblob ("moe" "moe~" "kyun!!"))
(loop 0 (- (length moeblob) 1)
	(id spit (at i moeblob)))

(thesis)